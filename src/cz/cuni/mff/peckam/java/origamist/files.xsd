<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" 
	xmlns:m="http://www.mff.cuni.cz/~peckam/java/origamist/files/"
	xmlns:c="http://www.mff.cuni.cz/~peckam/java/origamist/common/"
	xmlns:jxb="http://java.sun.com/xml/ns/jaxb"	
	jxb:version="2.0" 
	targetNamespace="http://www.mff.cuni.cz/~peckam/java/origamist/files/">	
	
	<import schemaLocation="common.xsd" namespace="http://www.mff.cuni.cz/~peckam/java/origamist/common/" />

	<annotation>
		<appinfo>			
			<jxb:schemaBindings>
				<jxb:package name="cz.cuni.mff.peckam.java.origamist.files.jaxb"/>
			</jxb:schemaBindings>
		</appinfo>
	</annotation>

    <element name="listing" type="m:Listing">
    </element>    
    
    <complexType name="Listing">
    	<sequence>
    		<element name="files" type="m:Files">
    			<annotation>
					<appinfo>
						<jxb:property 
							collectionType="cz.cuni.mff.peckam.java.origamist.utils.ChangeNotifyingList"/>
					</appinfo>
				</annotation>
    		</element>
    		<element name="categories" type="m:Categories" maxOccurs="1"
    			minOccurs="0">
    			<annotation>
					<appinfo>
						<jxb:property 
							collectionType="cz.cuni.mff.peckam.java.origamist.utils.ChangeNotifyingList"/>
					</appinfo>
				</annotation>
    		</element>
    	</sequence>
    </complexType>

    <complexType name="Files">
    	<sequence>
    		<element name="file" type="m:File" maxOccurs="unbounded" minOccurs="1"/>
    	</sequence>
    </complexType>

    <complexType name="File">
    	<sequence>
    		<element name="author" type="c:Author" maxOccurs="1" minOccurs="0"/>
    		<element name="name" type="c:LangString"
    			maxOccurs="unbounded" minOccurs="0">
    			<annotation>
					<appinfo>
						<jxb:property 
							collectionType="cz.cuni.mff.peckam.java.origamist.utils.ChangeNotifyingList"/>
					</appinfo>
				</annotation>
    		</element>
    		<element name="year" type="gYear" maxOccurs="1" minOccurs="0"/>
    		<element name="shortdesc" type="c:LangString"
    			maxOccurs="unbounded" minOccurs="0">
    			<annotation>
					<appinfo>
						<jxb:property 
							collectionType="cz.cuni.mff.peckam.java.origamist.utils.ChangeNotifyingList"/>
					</appinfo>
				</annotation>
    		</element>
    		<element name="license" type="c:License" maxOccurs="1" minOccurs="0"/>
    		<element name="original" type="anyURI" maxOccurs="1"
    			minOccurs="0"/>
    		<element name="thumbnail" type="c:Image" maxOccurs="1" minOccurs="0"/>
    	</sequence>
    	<attribute name="src" type="anyURI" use="required"/>
    </complexType>            

    <complexType name="Categories">
    	<sequence>
    		<element name="category" type="m:Category"
    			maxOccurs="unbounded" minOccurs="1">
    		</element>
    	</sequence>
    </complexType>

    <complexType name="Category">
    	<sequence>
    		<element name="name" type="c:LangString"
    			maxOccurs="unbounded" minOccurs="1">
    			<annotation>
					<appinfo>
						<jxb:property 
							collectionType="cz.cuni.mff.peckam.java.origamist.utils.ChangeNotifyingList"/>
					</appinfo>
				</annotation>
    		</element>
    		<element name="files" type="m:Files">
    			<annotation>
					<appinfo>
						<jxb:property 
							collectionType="cz.cuni.mff.peckam.java.origamist.utils.ChangeNotifyingList"/>
					</appinfo>
				</annotation>
    		</element>
    		<element name="categories" type="m:Categories">
    			<annotation>
					<appinfo>
						<jxb:property 
							collectionType="cz.cuni.mff.peckam.java.origamist.utils.ChangeNotifyingList"/>
					</appinfo>
				</annotation>
    		</element>
    	</sequence>    	
    	<attribute name="id" type="string" use="required"/>
    </complexType>
</schema>