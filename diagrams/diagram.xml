<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ns2:origami xmlns:ns2="http://www.mff.cuni.cz/~peckam/java/origamist/diagram/v2">
    <author>
        <name>Kawasaki</name>
        <homepage>http://www.mff.cuni.cz</homepage>
    </author>
    <name lang="en-US">Kawasaki rose</name>
    <creationDate>2010-01-01</creationDate>
    <shortdesc lang="en-US">A very nice, but hard rose model.</shortdesc>
    <shortdesc lang="cs-CZ">Opravdu překrásná růže, ale složitá.</shortdesc>
    <description lang="en-US">A very nice, but hard rose model. This has nothing to do with the czech film...</description>
    <description lang="cs-CZ">Opravdu překrásná růže, ale složitá. Nemá nic společného s českým filmem...</description>
    <license>
        <name>BSD</name>
        <content>You can do whatever you want except hiding or changing the authorship information.</content>
        <permission>export</permission>
        <permission>edit</permission>
        <permission>distribute</permission>
    </license>
    <thumbnail>
        <image type="jpg"></image>
    </thumbnail>
    <paper>
        <size>
            <width>200.0</width>
            <height>297.0</height>
            <unit>mm</unit>
            <referenceLength>297.0</referenceLength>
            <referenceUnit>mm</referenceUnit>
        </size>
        <color>
            <background>#fafaf2</background>
        </color>
        <cols>3</cols>
        <rows>3</rows>
    </paper>
    <model>
        <paper>
            <size>
                <width>20.0</width>
                <height>30.0</height>
                <unit>cm</unit>
                <referenceLength>30.0</referenceLength>
                <referenceUnit>cm</referenceUnit>
            </size>
            <colors>
                <background>#5555ff</background>
                <foreground>#ff8888</foreground>
            </colors>
            <weight>80.0</weight>
            <note lang="en-US">A common sheet of red A4 paper.</note>
            <note lang="cs-CZ">Obyčejný červený list papíru A4.</note>
        </paper>
        <steps>
            <step id="1">
                <description lang="en-US">Begin with a rectangle.</description>
                <description lang="cs-CZ">Začneme s obdélníkem.</description>
                <zoom>100.0</zoom>
            </step>
            <step id="2">
                <description lang="en-US">Rotate it 45° clockwise.</description>
                <description lang="cs-CZ">Otočte ho o 45° doprava.</description>
                <zoom>100.0</zoom>
                <rotateOperation type="ROTATE">
                    <angle>0.7853</angle>
                </rotateOperation>
            </step>
            <step id="3">
                <description lang="en-US">Make a valley fold parallel to the bottom of the paper.</description>
                <description lang="cs-CZ">Rovnoběžně se spodní hranou papíru udělejte údolní přehyb.</description>
                <zoom>100.0</zoom>
                <foldOperation type="VALLEY_FOLD">
                    <angle>1.0</angle>
                    <line>
                        <start>
                            <x>0.0</x>
                            <y>0.2</y>
                        </start>
                        <end>
                            <x>0.666666666</x>
                            <y>0.2</y>
                        </end>
                    </line>
                    <layer>1</layer>
                </foldOperation>
            </step>
            <step id="4">
                <description lang="en-US">Make a mountain fold parallel to the top of the paper.</description>
                <description lang="cs-CZ">Rovnoběžně s horní hranou papíru udělejte horní přehyb.</description>
                <zoom>100.0</zoom>
                <foldOperation type="MOUNTAIN_FOLD">
                    <angle>2.0</angle>
                    <line>
                        <start>
                            <x>0.0</x>
                            <y>0.8</y>
                        </start>
                        <end>
                            <x>0.666666666</x>
                            <y>0.8</y>
                        </end>
                    </line>
                    <layer>1</layer>
                </foldOperation>
            </step>
            <step id="5">
                <description lang="en-US">Rotate it 45° backwards.</description>
                <description lang="cs-CZ">Otočte ho zase o 45° zpět.</description>
                <zoom>100.0</zoom>
                <rotateOperation type="ROTATE">
                    <angle>-0.7853</angle>
                </rotateOperation>
            </step>
            <step id="6">
                <description lang="en-US">Turn the paper over.</description>
                <description lang="cs-CZ">Otočte papír vzhůru nohama.</description>
                <zoom>100.0</zoom>
                <turnOverOperation type="TURN_OVER"/>
            </step>
        </steps>
    </model>
</ns2:origami>
