<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ns2:origami xmlns:ns2="http://www.mff.cuni.cz/~peckam/java/origamist/diagram/v2">
    <author>
        <name>Martin Pecka</name>
        <homepage>http://www.mff.cuni.cz</homepage>
    </author>
    <name lang="en-US">Paper plane</name>
    <year>2011</year>
    <shortdesc lang="en-US">A simple paper plane.</shortdesc>
    <shortdesc lang="cs-CZ">Obyčejná šipka.</shortdesc>
    <description lang="en-US">A simple paper plane every child knows.</description>
    <description lang="cs-CZ">Obyčejná šipka, kterou umí každé dítě.</description>
    <license>
        <name>BSD</name>
        <content>You can do whatever you want except hiding or changing the authorship information.</content>
        <permission>export</permission>
        <permission>edit</permission>
        <permission>distribute</permission>
    </license>
    <thumbnail>
        <image type="jpg"></image>
    </thumbnail>
    <paper>
        <size>
            <width>200.0</width>
            <height>297.0</height>
            <unit>mm</unit>
            <referenceLength>297.0</referenceLength>
            <referenceUnit>mm</referenceUnit>
        </size>
        <color>
            <background>#fafaf2</background>
        </color>
        <cols>3</cols>
        <rows>3</rows>
    </paper>
    <model>
        <paper>
            <size>
                <width>210.0</width>
                <height>297.0</height>
                <unit>mm</unit>
                <referenceLength>297.0</referenceLength>
                <referenceUnit>mm</referenceUnit>
            </size>
            <colors>
                <background>#ffffff</background>
                <foreground>#ffffff</foreground>
            </colors>
            <weight>80.0</weight>
            <note lang="en-US">A common sheet of white A4 paper.</note>
            <note lang="cs-CZ">Obyčejný bílý list papíru A4.</note>
        </paper>
        <steps>
            <step id="1">
                <description lang="en-US">Begin with plain A4 paper.</description>
                <description lang="cs-CZ">Začneme s listem papíru A4.</description>
                <zoom>190.0</zoom>
            </step>
            <step id="2">
                <description lang="en-US">Fold and unfold around the centers of the shorter sides.</description>
                <description lang="cs-CZ">Vytvořte přehyb mezi středy kratšich stran.</description>
                <zoom>170.0</zoom>
                <foldUnfoldOperation type="VALLEY_MOUNTAIN_FOLD_UNFOLD">
                    <line>
                        <start>
                            <x>0.35353535353535354</x>
                            <y>0.0</y>
                        </start>
                        <end>
                            <x>0.35353535353535354</x>
                            <y>1.0</y>
                        </end>
                    </line>
                    <layer>1</layer>
                </foldUnfoldOperation>
            </step>
            <step id="3">
                <description lang="en-US">Fold the top flaws to the center line.</description>
                <description lang="cs-CZ">Horní chlopně přehněte ke středu.</description>
                <colspan>2</colspan>
                <rowspan>2</rowspan>
                <zoom>200.0</zoom>
                <foldOperation type="VALLEY_FOLD">
                    <angle>3.141592</angle>
                    <line>
                        <start>
                            <x>0.0</x>
                            <y>0.6464646464646464</y>
                        </start>
                        <end>
                            <x>0.35353535353535354</x>
                            <y>1.0</y>
                        </end>
                    </line>
                    <refPoint>
                        <x>0.0</x>
                        <y>1.0</y>
                    </refPoint>
                    <layer>1</layer>
                </foldOperation>
                <repeatOperation displayed="true" type="REPEAT_ACTION">
                    <foldOperation type="VALLEY_FOLD">
                        <angle>3.141592</angle>
                        <line>
                            <start>
<x>0.7070707070707071</x>
<y>0.6464646464646464</y>
                            </start>
                            <end>
<x>0.35353535353535354</x>
<y>1.0</y>
                            </end>
                        </line>
                        <refPoint>
                            <x>0.7070707070707071</x>
                            <y>1.0</y>
                        </refPoint>
                        <layer>1</layer>
                    </foldOperation>
                </repeatOperation>
            </step>
            <step id="4">
                <description lang="en-US">Rotate it 90° right.</description>
                <description lang="cs-CZ">Otočte papír o 90° doprava.</description>
                <zoom>240.0</zoom>
                <rotateOperation type="ROTATE">
                    <angle>1.570796</angle>
                </rotateOperation>
            </step>
            <step id="5">
                <description lang="en-US">Fold along the center line.</description>
                <description lang="cs-CZ">Přehněte podél střední čáry.</description>
                <zoom>270.0</zoom>
                <foldOperation type="VALLEY_FOLD">
                    <angle>3.0</angle>
                    <line>
                        <start>
                            <x>0.35353535353535354</x>
                            <y>0.0</y>
                        </start>
                        <end>
                            <x>0.35353535353535354</x>
                            <y>1.0</y>
                        </end>
                    </line>
                    <layer>3</layer>
                </foldOperation>
            </step>
            <step id="6">
                <description lang="en-US">Rotate the paper 180 degrees right and then turn over.</description>
                <description lang="cs-CZ">Otočte papír o 180° doprava a pak vzhůru nohama.</description>
                <zoom>270.0</zoom>
                <rotateOperation type="ROTATE">
                    <angle>3.1415926</angle>
                </rotateOperation>
                <turnOverOperation type="TURN_OVER"/>
            </step>
            <step id="7">
                <description lang="en-US">Fold the wings.</description>
                <description lang="cs-CZ">Ohněte křídla</description>
                <zoom>270.0</zoom>
                <foldOperation type="VALLEY_FOLD">
                    <angle>1.5707963</angle>
                    <line>
                        <start>
                            <x>0.25</x>
                            <y>0.0</y>
                        </start>
                        <end>
                            <x>0.25</x>
                            <y>0.8824</y>
                        </end>
                    </line>
                    <refPoint>
                        <x>0.0</x>
                        <y>0.0</y>
                    </refPoint>
                    <layer>1</layer>
                    <layer>2</layer>
                </foldOperation>
                <foldOperation type="MOUNTAIN_FOLD">
                    <angle>1.5707963</angle>
                    <line>
                        <start>
                            <x>0.4570707070707</x>
                            <y>0.0</y>
                        </start>
                        <end>
                            <x>0.4570707070707</x>
                            <y>0.8824</y>
                        </end>
                    </line>
                    <refPoint>
                        <x>0.7070707070707071</x>
                        <y>0.0</y>
                    </refPoint>
                    <layer>1</layer>
                    <layer>2</layer>
                </foldOperation>
            </step>
            <step id="8">
                <description lang="en-US">Do inside reverse fold on the back and outside reverse fold on the front of the plane.</description>
                <description lang="cs-CZ">Prohněte zadní část šipky dovnitř a přední ven.</description>
                <colspan>2</colspan>
                <zoom>270.0</zoom>
                <reverseFoldOperation type="INSIDE_REVERSE_FOLD">
                    <line>
                        <start>
                            <x>0.25</x>
                            <y>0.0</y>
                        </start>
                        <end>
                            <x>0.35353535353535354</x>
                            <y>0.08333333</y>
                        </end>
                    </line>
                    <refLine>
                        <start>
                            <x>0.35353535353535354</x>
                            <y>0.08333333</y>
                        </start>
                        <end>
                            <x>0.35353535353535354</x>
                            <y>0.0</y>
                        </end>
                    </refLine>
                    <layer>1</layer>
                </reverseFoldOperation>
                <reverseFoldOperation type="OUTSIDE_REVERSE_FOLD">
                    <line>
                        <start>
                            <x>0.25</x>
                            <y>0.8964646464646464</y>
                        </start>
                        <end>
                            <x>0.35353535353535354</x>
                            <y>0.8964646464646464</y>
                        </end>
                    </line>
                    <refLine>
                        <start>
                            <x>0.35353535353535354</x>
                            <y>1.0</y>
                        </start>
                        <end>
                            <x>0.35353535353535354</x>
                            <y>0.8964646464646464</y>
                        </end>
                    </refLine>
                    <layer>1</layer>
                    <layer>2</layer>
                </reverseFoldOperation>
            </step>
            <step id="9">
                <description lang="en-US">Now watch where will points A-D move.</description>
                <description lang="cs-CZ">Sledujte, kam se hýbou body A-D.</description>
                <zoom>270.0</zoom>
                <markerOperation type="MARKER">
                    <text>A</text>
                    <refPoint>
                        <x>0.0</x>
                        <y>0.0</y>
                    </refPoint>
                    <stepsToHide>3</stepsToHide>
                </markerOperation>
                <markerOperation type="MARKER">
                    <text>B</text>
                    <refPoint>
                        <x>0.0</x>
                        <y>1.0</y>
                    </refPoint>
                    <stepsToHide>3</stepsToHide>
                </markerOperation>
                <markerOperation type="MARKER">
                    <text>C</text>
                    <refPoint>
                        <x>0.7070707070707071</x>
                        <y>0.0</y>
                    </refPoint>
                    <stepsToHide>1</stepsToHide>
                </markerOperation>
                <markerOperation type="MARKER">
                    <text>D</text>
                    <refPoint>
                        <x>0.7070707070707071</x>
                        <y>1.0</y>
                    </refPoint>
                    <stepsToHide>1</stepsToHide>
                </markerOperation>
            </step>
            <step id="10">
                <description lang="en-US">Fine-tune wings with thunderbolt folds.</description>
                <description lang="cs-CZ">Vytuňte křídla pomocí cik-cak skladů.</description>
                <zoom>270.0</zoom>
                <thunderboltFoldOperation type="THUNDERBOLT_FOLD">
                    <angle>3.14</angle>
                    <line>
                        <start>
                            <x>0.0</x>
                            <y>0.065</y>
                        </start>
                        <end>
                            <x>0.065</x>
                            <y>0.0</y>
                        </end>
                    </line>
                    <layer>1</layer>
                    <secondAngle>3.14</secondAngle>
                    <secondLine>
                        <start>
                            <x>0.0</x>
                            <y>0.05</y>
                        </start>
                        <end>
                            <x>0.05</x>
                            <y>0.0</y>
                        </end>
                    </secondLine>
                </thunderboltFoldOperation>
                <thunderboltFoldOperation type="THUNDERBOLT_FOLD">
                    <angle>3.14</angle>
                    <line>
                        <start>
                            <x>0.7070707070707071</x>
                            <y>0.065</y>
                        </start>
                        <end>
                            <x>0.6420707070707071</x>
                            <y>0.0</y>
                        </end>
                    </line>
                    <layer>1</layer>
                    <secondAngle>3.14</secondAngle>
                    <secondLine>
                        <start>
                            <x>0.7070707070707071</x>
                            <y>0.05</y>
                        </start>
                        <end>
                            <x>0.657070707070707</x>
                            <y>0.0</y>
                        </end>
                    </secondLine>
                </thunderboltFoldOperation>
            </step>
            <step id="11">
                <description lang="cs-CZ">Vltačte čumák dovnitř.</description>
                <description lang="en-US">Press the nose inside.</description>
                <zoom>270.0</zoom>
                <crimpFoldOperation type="INSIDE_CRIMP_FOLD">
                    <line>
                        <start>
                            <x>0.35353535353535354</x>
                            <y>0.9746419096516249</y>
                        </start>
                        <end>
                            <x>0.4319066764856831</x>
                            <y>0.8964646464646517</y>
                        </end>
                    </line>
                    <refLine>
                        <start>
                            <x>0.35353535353535354</x>
                            <y>1.0</y>
                        </start>
                        <end>
                            <x>0.3535353535353535</x>
                            <y>0.8964646464646461</y>
                        </end>
                    </refLine>
                    <layer>5</layer>
                    <layer>6</layer>
                    <layer>7</layer>
                    <layer>8</layer>
                    <secondLine>
                        <start>
                            <x>0.4053030303030267</x>
                            <y>0.8964646464646497</y>
                        </start>
                        <end>
                            <x>0.3535353535353535</x>
                            <y>0.9482323232323231</y>
                        </end>
                    </secondLine>
                </crimpFoldOperation>
            </step>
            <step id="12">
                <description lang="cs-CZ">A máme hotovo.</description>
                <description lang="en-US">And that's it.</description>
                <zoom>270.0</zoom>
            </step>
        </steps>
    </model>
</ns2:origami>
